{
  "manifest_version": 3,
  "name": "book-pdf",
  "description": "yay",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "activeTab",
    "tabCapture",
    "scripting",
    "debugger"
  ],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "build/js/background.bundle.js"
  },
  "action": {},
  "icons": {
    "16": "build/images/icon19.png",
    "48": "build/images/icon48.png",
    "128": "build/images/icon128.png"
  }
}
